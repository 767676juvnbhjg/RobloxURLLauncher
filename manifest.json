{
  "manifest_version": 2,
  "name": "Roblox URL Launcher",
  "version": "1.4",
  "description": "Joins a game, specific server or opens studio basing on url parameters.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://*.roblox.com/home*", "*://*.roblox.com/games*", "*://*.roblox.com/groups*"],
      "run_at": "document_end",
      "js": ["index.js"]
    }
  ]
}